{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Corefront CLI Module",
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "module": "NodeNext",
    "strict": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "paths": {
      "~/*": [
        "./*"
      ]
    }
  }
}
