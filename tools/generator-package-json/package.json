{
  "name": "@corefront/generator-package-json",
  "version": "1.0.0",
  "description": "Corefront package.json generator",
  "license": "ISC",
  "author": "Vyacheslav Shimarulin",
  "type": "module",
  "types": "./dist/main.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.mts",
        "import": "./dist/main.mjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "postinstall": "[ -d \"./dist\" ] || npm run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^4.3.2",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@corefront/cli": "workspace:*",
    "@corefront/tsconfig-preset-es-module": "workspace:*",
    "@types/ramda": "^0.29.12",
    "concurrently": "^8.2.2",
    "tsc-alias": "^1.8.8",
    "type-fest": "^4.15.0"
  }
}
