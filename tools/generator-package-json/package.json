{
  "name": "@corefront/generator-package-json",
  "version": "1.0.0",
  "description": "Corefront package.json generator",
  "license": "ISC",
  "author": "Vyacheslav Shimarulin",
  "type": "module",
  "types": "./src/main.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/main.mts",
        "import": "./dist/main.mjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "swc --delete-dir-on-start --strip-leading-paths --out-file-extension mjs -d dist src",
    "dev": "pnpm run build --watch",
    "postinstall": "pnpm run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^4.3.2",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@corefront/cli": "workspace:*",
    "@corefront/tsconfig-preset-tools": "workspace:*",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.11",
    "@types/ramda": "^0.29.12",
    "chokidar": "^3.6.0",
    "type-fest": "^4.15.0"
  }
}
